Algoritmo ejercicio_6
	Definir n Como Entero;
	n = 0;
	
	Imprimir "Almacenar en un vector";
	Imprimir "Ingrese la dimensión del vector: ";
	Leer n;
	
	Dimension v1[n];
	
	Imprimir "Ingrese valor para posición 1: ";
	Leer v1[1];
	Imprimir "Ingrese valor para posición 2: ";
	Leer v1[2];
	Imprimir "Ingrese valor para posición 3: ";
	Leer v1[3];
	Imprimir "Ingrese valor para posición ", n, ": ";
	Leer v1[n];
	
	Imprimir "Fin Proceso";
FinAlgoritmo
